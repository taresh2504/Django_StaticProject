<script>
  

window.openModal = function () {
  const modal = document.getElementById('myModal');
  if (!modal) return;

  modal.style.display = 'flex';
  modal.classList.add('active');
  document.body.classList.add('popup-open');
};

window.closeModal = function () {
  const modal = document.getElementById('myModal');
  if (!modal) return;

  modal.style.display = 'none';
  modal.classList.remove('active');
  document.body.classList.remove('popup-open');
};

window.toggleTab = function (tabIndex) {
  const tabButtons = document.querySelectorAll('.tab-btn');
  const tabContents = document.querySelectorAll('.tab-content_new');

  tabContents.forEach(function (tab) {
    tab.style.display = 'none';
  });

  tabButtons.forEach(function (btn) {
    btn.classList.remove('active');
  });

  const activeTab = document.getElementById('tab' + tabIndex);
  if (activeTab) activeTab.style.display = 'block';

  if (tabButtons[tabIndex - 1]) {
    tabButtons[tabIndex - 1].classList.add('active');
  }
};
document.addEventListener('click', function (e) {
  const modal = document.getElementById('myModal');
  if (!modal) return;

  if (e.target === modal) {
    window.closeModal();
  }
});
document.addEventListener('keydown', function (e) {
  if (e.key === 'Escape') {
    window.closeModal();
  }
});
</script> 






