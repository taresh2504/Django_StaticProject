    transition: transform 0.3s ease-in-out;
  }

  .cstm_modal.active {
    display: block;
    animation: slideUp 0.3s ease forwards;
  }

  @keyframes slideUp {
    to {
      transform: translateY(0);
    }
  }

  .cstm_modal_content {
    position: relative;
    padding: 10px 20px 0 20px;
